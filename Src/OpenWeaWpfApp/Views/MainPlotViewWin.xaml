      <Grid.Resources>
        <Color x:Key="Vgn">#08f</Color>
        <Color x:Key="Mis">#080</Color>
        <Color x:Key="d00">#d00</Color>
        <Color x:Key="b80">#b80</Color>
        <Color x:Key="Phc">#e0e</Color>
